{
  "name": "uguee",
  "version": "1.0.0",
  "description": "Aplicación web y móvil de Uguee",
  "private": true,
  "workspaces": [
    "web",
    "mobile"
  ],
  "scripts": {
    "web:install": "cd web && npm install",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "web:preview": "cd web && npm run preview",
    "mobile:install": "cd mobile && npm install",
    "mobile:start": "cd mobile && npm start",
    "mobile:android": "cd mobile && npm run android",
    "mobile:ios": "cd mobile && npm run ios",
    "mobile:web": "cd mobile && npm run web",
    "install:all": "npm run web:install && npm run mobile:install",
    "dev": "concurrently \"npm run web:dev\" \"npm run mobile:start\"",
    "lint:web": "cd web && npm run lint",
    "build:all": "npm run web:build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "react",
    "react-native",
    "expo",
    "typescript",
    "vite",
    "tailwind"
  ],
  "author": "",
  "license": "ISC"
} 